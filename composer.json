{
  "name": "kriss/package-check",
  "description": "package checker for packagist",
  "keywords": ["yii2", "package-check", "packagist"],
  "version": "1.0.1",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "kriss",
      "email": "462679766@qq.com"
    }
  ],
  "support": {
    "source": "https://github.com/krissss/package-check"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0",
    "dmstr/yii2-adminlte-asset": "^2.6",
    "kartik-v/yii2-datecontrol": "^1.9",
    "kartik-v/yii2-dynagrid": "^1.4.7",
    "kriss/yii2-adminlte-iframe-mode": "^2.0",
    "kriss/yii2-common-class": "^2.1",
    "kriss/yii2-log-reader": "^2.2",
    "vlucas/phpdotenv": "^2.4",
    "yiisoft/yii2": "2.0.15.1",
    "yiisoft/yii2-bootstrap": "^2.0",
    "yiisoft/yii2-httpclient": "^2.0",
    "yiisoft/yii2-queue": "^2.1"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "^2.0",
    "yiisoft/yii2-gii": "^2.0"
  },
  "config": {
    "process-timeout": 1800,
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "npm-asset-repository": "https://registry.npm.taobao.org"
  },
  "scripts": {
    "post-create-project-cmd": "php -r \"file_exists('.env-project') || copy('.env-project-example', '.env-project');\""
  },
  "repositories": {
    "composer": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  }
}
